();\n   reader.onload = (e) => {\n    setImage(e.target.result);\n    setValue('image', file);\n   };\n   reader.readAsDataURL(file);\n  } else {\n   setImage('');\n   setValue('image', '');\n  }\n };\n\n const onChangeValue = (e, name) => {\n  setValue(name, e.target.value);\n };\n\n const cancel = () => history.push('/dish');\n\n const onSubmit = (data, e) => {\n  e.preventDefault();\n  const formData = new FormData();\n  formData.append('image', data.image);\n  formData.append('price', data.price);\n  formData.append('store_name', data.store_name);\n  formData.append('sales_name', data.sales_name);\n  formData.append('address', data.address);\n  formData.append('description', data.description);\n  formData.append('phone', data.phone);\n\n  const config = {\n   headers: {\n    'Content-Type': 'multipart/form-data',\n   },\n   body: formData,\n  };\n\n  if (id === 'create') {\n   InvoiceService.create(formData, config).then((res) => {\n    setImage('');\n    // clear field.\n    // e.target.reset();\n    history.push('/invoice');\n    console.log('data create success: ', res);\n   });\n  } else {\n   InvoiceService.updateDataById(id, config, formData).then((res) => {\n    setImage('');\n    history.push('/invoice');\n    console.log('data update success: ', res);\n   });\n  }\n };\n\n const getTitle = () => {\n  if (id === 'create') {\n   return <h3 className=\"text-center\">Add Invoice</h3>;\n  } else {\n   return <h3 className=\"text-center\">Update Invoice</h3>;\n  }\n };\n\n return (\n  <div className=\"container\" style={{ marginTop: '8rem' }}>\n   <div className=\"row\">\n    <div className=\"card col-md-6 offset-md-3 offset-md-3 pt-4\">\n     {getTitle()}\n     <div className=\"card-body\">\n      <form onSubmit={handleSubmit(onSubmit)}>\n       <div className=\"form-group\">\n        <label> Image: </label>\n        <input\n         type=\"file\"\n         className=\"form-control\"\n         {...register('image', { required: true })}\n         onChange={(e) => changeimageURLHandler(e)}\n        />\n       </div>\n       {image && (\n        <div>\n         <img src={image} alt=\"display\" style={{ width: '100px', marginBottom: '1rem' }} />\n        </div>\n       )}\n       <div className=\"form-group\">\n        <label> Price: </label>\n        <input\n         type=\"number\"\n         className=\"form-control\"\n         {...register('price', { required: true })}\n         onChange={(e) => onChangeValue(e, 'price')}\n        />\n       </div>\n\n       <div className=\"form-group\">\n        <label> Store Name: </label>\n        <input\n         type=\"text\"\n         className=\"form-control\"\n         {...register('store_name', { required: true })}\n         onChange={(e) => onChangeValue(e, 'store_name')}\n        />\n       </div>\n\n       <div className=\"form-group\">\n        <label> Sales Name: </label>\n        <input\n         type=\"text\"\n         className=\"form-control\"\n         {...register('sales_name', { required: true })}\n         onChange={(e) => onChangeValue(e, 'sales_name')}\n        />\n       </div>\n\n       <div className=\"form-group\">\n        <label> Address: </label>\n        <textarea\n         className=\"form-control\"\n         rows=\"5\"\n         {...register('address', { required: true })}\n         onChange={(e) => onChangeValue(e, 'address')}\n        />\n       </div>\n\n       <div className=\"form-group\">\n        <label> Description: </label>\n        <textarea\n         className=\"form-control\"\n         rows=\"5\"\n         {...register('description', { required: true })}\n         onChange={(e) => onChangeValue(e, 'description')}\n        />\n       </div>\n\n       <div className=\"form-group\">\n        <label> Phone: </label>\n        <input\n         type=\"number\"\n         className=\"form-control\"\n         {...register('phone', { required: true })}\n         onChange={(e) => onChangeValue(e, 'phone')}\n        />\n       </div>\n\n       <button className=\"btn btn-success\" type=\"submit\">\n        Save\n       </button>\n       <button className=\"btn btn-danger\" onClick={cancel} style={{ marginLeft: '10px' }}>\n        Cancel\n       </button>\n      </form>\n     </div>\n    </div>\n   </div>\n  </div>\n );\n};\n\nexport default CreateInvoiceComponent;\n","/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Pages/Checkout/checkoutList.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Pages/Invoice/list.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Pages/Invoice/view.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Components/Footer/footer.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Components/Navbar/Navbar.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Components/Form/signUp.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Components/Form/signIn.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/action/types.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Components/Header.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Components/Navbar/navbarSlide.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Services/dishServices.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Services/invoices.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Services/dishStock.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Services/dishCheckout.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/reducers/index.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/Components/Product/index.js",[],"/home/bill/Documents/Web_App/KP-Manajemen/frontend-2/src/reducers/error.js",["141"],"import { GET_ERRORS } from \"../action/types\";\n\nconst initialState = {};\nexport default function (state = initialState, action) {\n  switch (action.type) {\n    case GET_ERRORS:\n      return action.payloa