nsole.log(\"error\", error);\r\n         })\r\n }, [id]);\r\n\r\n//  handle image upload when cretae and update image.\r\n  const changeimageURLHandler = (event) => {\r\n  const file = event.target.files[0];\r\n\r\n  if (event.target.files && event.target.files[0]) {\r\n   let reader = new FileReader();\r\n   reader.onload = (e) => {\r\n    setImage(e.target.result);\r\n    setValue('image', file);\r\n   };\r\n   reader.readAsDataURL(file);\r\n  } else {\r\n   setImage('');\r\n   setValue('image', '');\r\n  }\r\n };\r\n\r\n const onChangeValue = (e, name) => {\r\n  setValue(name, e.target.value);\r\n };\r\n\r\n const cancel = () => history.push('/dish');\r\n \r\n  const onSubmit = (data, e) => {\r\n  const formData = new FormData();\r\n  formData.append('image', data.image);\r\n  formData.append('name', data.name);\r\n  formData.append('categories', data.categories);\r\n  formData.append('description', data.description);\r\n  formData.append('quantity', data.quantity);\r\n    console.log( data, '1. ==================')\r\n  const config = {\r\n   headers: {\r\n    'Content-Type': 'multipart/form-data',\r\n   },\r\n   body: formData,\r\n  };\r\n\r\n  if (id === 'create') {\r\n   DishService.create(formData, config).then((res) => {\r\n    setImage('');\r\n    // clear field.\r\n    // e.target.reset();\r\n    history.push('/dish');\r\n    console.log('data create success: ', res);\r\n   });\r\n  } else {\r\n    axios.put(`http://localhost:8000/dish/edit/${id}`, formData, config).then((res) => {\r\n    setImage('')\r\n    history.push('/dish');\r\n    console.log('data update success: ', res);\r\n   });\r\n  }\r\n };\r\n\r\n // change title.\r\n const getTitle = () => {\r\n     if (id === \"create\") {\r\n         return <h3 className=\"text-center\">Add Dish</h3>\r\n     }\r\n     else {\r\n        return <h3 className=\"text-center\">Update Dish</h3>\r\n     }\r\n }\r\n \r\n return (\r\n  <div className=\"container\" style={{ marginTop: '8rem' }}>\r\n   <div className=\"row\">\r\n    <div className=\"card col-md-6 offset-md-3 offset-md-3 pt-4\">\r\n        {getTitle()}\r\n     <div className=\"card-body\">\r\n      <form onSubmit={handleSubmit(onSubmit)}>\r\n       <div className=\"form-group\">\r\n        <label> Image: </label>\r\n        <input\r\n         type=\"file\"\r\n         className=\"form-control\"\r\n         defaultValue={data.image}\r\n         {...register('image', { required: true })}\r\n         onChange={(e) => changeimageURLHandler(e)}\r\n        />\r\n       </div>\r\n       \r\n       {image ? (\r\n        <div>\r\n         <img src={image} alt=\"display\" style={{width: \"100px\", marginBottom:\"1rem\"}}/>\r\n        </div>\r\n       ) : data.image ? (\r\n        <div>\r\n         <img src={`http://localhost:8000/${data.image}`} alt=\"display\" style={{width: \"100px\", marginBottom:\"1rem\"}}/>\r\n        </div>\r\n       ) : 'Upload image!'\r\n    }\r\n       {console.log(data, '2.===============================================')}\r\n\r\n       <div className=\"form-group\">\r\n        <label> Dish Name: </label>\r\n        <input\r\n         type=\"text\"\r\n         className=\"form-control\"\r\n         defaultValue={data.name}\r\n         {...register('name', { required: true })}\r\n         onChange={(e) => onChangeValue(e, 'name')}\r\n        />\r\n       </div>\r\n\r\n       <div className=\"input-group mb-3\">\r\n       <div className=\"input-group\">\r\n            <label>Categories:</label>\r\n        </div>\r\n        <select className=\"custom-select\" id=\"inputGroupSelect02\"\r\n        defaultValue={data.categories}\r\n        {...register('categories', { required: true })}\r\n        onChange={(e) => onChangeValue(e, 'categories')}>\r\n            <option>Choose Categories!</option>\r\n            <option>One</option>\r\n            <option>Two</option>\r\n            <option>Three</option>\r\n        </select>\r\n        </div>\r\n\r\n       <div className=\"form-group\">\r\n        <label> Description: </label>\r\n        <textarea className=\"form-control\" \r\n         rows=\"5\"\r\n         defaultValue={data.description}\r\n         {...register('description', { required: true })}\r\n         onChange={(e) => onChangeValue(e, 'description')}\r\n        />\r\n       </div>\r\n\r\n       <div className=\"form-group\">\r\n        <label> Quantity: </label>\r\n        <input\r\n         type=\"number\"\r\n         className=\"form-control\"\r\n         defaultValue={data.quantity}\r\n         {...register('quantity', { required: true })}\r\n         onChange={(e) => onChangeValue(e, 'quantity')}\r\n        />\r\n       </div>\r\n\r\n       <button className=\"btn btn-success\" type=\"submit\">\r\n        Save\r\n       </button>\r\n       <button className=\"btn btn-danger\" onClick={cancel} style={{ marginLeft: '10px' }}>\r\n        Cancel\r\n       </button>\r\n      </form>\r\n     </div>\r\n    </div>\r\n   </div>\r\n  </div>\r\n );\r\n};\r\n\r\nexport default CreateDishComponent;","C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Pages\\Dish\\dishList.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Pages\\Dish\\dishView.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Pages\\Stock\\stockUpdate.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Pages\\Stock\\stockView.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Pages\\Checkout\\checkoutUpdate.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Pages\\Checkout\\checkoutList.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Pages\\Stock\\stockList.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Components\\Navbar\\Navbar.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Components\\Footer\\footer.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\frontend-2\\src\\Components\\Form\\signUp.js",[],"C:\\Users\\ASUS\\Downloads\\KP-Manajemen\\fronte